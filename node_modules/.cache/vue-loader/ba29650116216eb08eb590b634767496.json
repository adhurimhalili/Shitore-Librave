tion\").value);\r\n           bodyFormData.append(\"image\", image.files[0]);\r\n\r\n          axios.post(`http://localhost:4000/books`, bodyFormData)\r\n      .then(response => {\r\n        //console.log(response);\r\n        this.$router.push({\r\n          name: 'home',\r\n          //params: { id: response.data._id }\r\n        })\r\n        })\r\n        \r\n    }\r\n  }\r\n};\r\n</script>"]}]}